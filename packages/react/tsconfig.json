{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"jsx": "react",
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"declaration": true,
		"declarationDir": "types",
		"emitDeclarationOnly": true
	}
}
